<h1>Login</h1>
<form method = 'post'>
	<input type = 'text' name = 'user[login]' placeholder = 'Login'>
	<input type = 'password' name = 'user[passwd]' placeholder = 'Password'>
	<input type = 'submit' value = 'OK'>
	<br />
	<% if (typeof misc != 'undefined' && misc == 'exist_log') { %>
		Wrong login
	<% } %>
	<% if (typeof misc != 'undefined' && misc == 'exist_pwd') { %>
		Wrong password or login
	<% } %>
</form>
